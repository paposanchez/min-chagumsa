{
    "name": "chali5124/zlara",
    "description": "Laravel CMS Skelecton Framework",
    "keywords": ["zlara", "framework", "laravel"],
    "license": "Copyright by LEE CHAN RIN",
    "authors": [
        {
            "name": "LEE CHAN RIN",
            "email": "chali5124@gmail.com"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-debugbar": "^2.3",
        "baum/baum": "~1.1",
        "davejamesmiller/laravel-breadcrumbs": "^3.0.1",
        "intervention/image": "^2.3.11",
        "jenssegers/agent": "^2.3",
        "laracasts/flash": "^2.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "maatwebsite/excel": "^2.1",
        "nesbot/carbon": "^1.22",
        "zizaco/entrust": "5.2.x-dev",
        "spatie/laravel-activitylog": "^1.12",
        "rtconner/laravel-tagging": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
